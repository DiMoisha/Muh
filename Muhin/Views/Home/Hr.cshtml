@model List<Muh.Models.HourView>

@{
    ViewBag.Title = "Выпуск";
}

<h2>Выберите Категорию</h2>

@using (Html.BeginForm())
{
    @Html.AntiForgeryToken()

    @Html.DropDownList("cat", (SelectList)ViewData["categ"], new { onchange = "$(this.form).submit();" })
}

<h2>
    Выпуск по часам
    c @ViewBag.Beg по c @ViewBag.end
</h2>

<table class="table">
    <tr>
        <th>
            Час
        </th>
        <th>
            Кол-во
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.txt)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.kol)
            </td>
        </tr>
    }

</table>
